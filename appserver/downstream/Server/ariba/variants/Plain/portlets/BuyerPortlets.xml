<PortletProviders>
    <PortletProvider stringFile="&lt;ProviderURL&gt;/ad/stringFile/PortletProviderActions?stringFile=ariba.html.buyerportlets">
        <UniqueName>AribaBuyer</UniqueName>
        <DisplayName>@BuyerAppTitle</DisplayName>
        <Description>@BuyerAppDesc</Description>
        <BusinessKey>Buyer</BusinessKey>
        <HttpURL>&HttpURL;</HttpURL>
        <ServerURL>&ServerURL;</ServerURL>
        <InstalledModules>&InstalledModules;</InstalledModules>
        <Permissions></Permissions>
        <Portlets>
            <Portlet>
                <UniqueName>SavedSearch</UniqueName>
                <DisplayName>@SavedSearchTitle</DisplayName>
                <Description>@SavedSearchDesc</Description>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.orms.portlets.WatchListController</Controller>
                <ContentType>DataTable</ContentType>
                <IsSingleton>false</IsSingleton>
                <IsEditable>true</IsEditable>
                <IsTOC>false</IsTOC>
                <SetupRequired>true</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>BuyerActions</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.buyer.portlets.BuyerActionsController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>Actions</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>false</IsEditable>
                <SetupRequired>true</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>BuyerSearchActions</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.orms.portlets.BuyerSearchActionsPortletController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>Actions</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>false</IsEditable>
                <SetupRequired>true</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
               <UniqueName>BuyerSearch</UniqueName>
               <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.orms.portlets.BuyerSearchPortletController</Controller>
               <ContentType>AggregationSource</ContentType>
               <AggregationDataType>Search</AggregationDataType>
               <InvalidateOnLogin>true</InvalidateOnLogin>
               <IsSingleton>true</IsSingleton>
               <IsEditable>false</IsEditable>
               <SetupRequired>false</SetupRequired>
               <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>BuyerTodo</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.orms.portlets.BuyerToDoPortletController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>ToDo</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>false</IsEditable>
                <SetupRequired>false</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>BuyerMyDocs</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.orms.portlets.BuyerMyDocsPortletController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>MyDocs</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>false</IsEditable>
                <SetupRequired>false</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>BuyerRecentlyViewed</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.orms.portlets.BuyerRecentlyViewedController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>RecentlyViewed</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>false</IsEditable>
                <SetupRequired>true</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>BuyerPreferences</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/ariba.htmlui.buyer.portlets.BuyerUserPreferencesPortletController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>Actions</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>false</IsEditable>
                <SetupRequired>false</SetupRequired>
                <Permissions></Permissions>
            </Portlet>
            <Portlet>
                <UniqueName>NewsSource</UniqueName>
                <Controller>&lt;ProviderURL&gt;/ad/&lt;action&gt;/NewsSourcePortletController</Controller>
                <ContentType>AggregationSource</ContentType>
                <AggregationDataType>News</AggregationDataType>
                <IsSingleton>true</IsSingleton>
                <IsEditable>true</IsEditable>
                <SetupRequired>true</SetupRequired>
                <Permissions>
                </Permissions>
            </Portlet>
        </Portlets>
        <DocTypes>
            <!--- Note: this is a fake DocType to control rank of Catalog item in the Search portlet -->
            <DocType>
                <UniqueName>ariba.purchasing.core.Catalog</UniqueName>
                <DisplayName>@CatalogDocType</DisplayName>
                <Description>@CatalogDocTypeDesc</Description>
                <ActionRank>5</ActionRank>
                <Permissions>
                    <String>CreateRequisition</String>
                </Permissions>
            </DocType>
        </DocTypes>
    </PortletProvider>
</PortletProviders>
